{
  "name": "base-nodejs-mongo-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run run-dev",
    "clear": "rm -rf dist || rmdir /s dist",
    "build": "npm run clear && mkdir dist && babel app -s -d dist",
    "release": "npm run build",
    "production": "npm run build && nodemon bin/production",
    "run-prod": "node bin/production",
    "run-dev": "NODE_ENV=development nodemon --ext js,json,yaml bin/dev",
    "postinstall": "npm run build"
  },
  "dependencies": {
